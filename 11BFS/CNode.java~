public class CNode{
    public int a, b;
    private CNode next;
    public CNode(int x, int y, CNode Paul){
	System.out.println(""+x+","+y);
	a = x;
	b = y;
	next = Paul;
    }
    public int getX(){
	return a;
    }
    public int getY(){
	System.out.println(b);
	return b;
    }
    public CNode getNext(){
	return next;
    }
    public boolean hasNext(){
	return next != null;
    }
}
